{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "eslint.format.enable": true,
    "prettier.requireConfig": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/cjm": true,
        "**/coverage": true,
        "**/esm": true
    }
}
