{
  "name": "react-clean-use-case",
  "version": "1.0.15",
  "description": "React mappers for clean-reactive-architecture",
  "main": "cjm/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "keywords": [
    "react",
    "usecase",
    "clean-use-case",
    "rxjs"
  ],
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "start": "webpack server --config playground/webpack.config.js",
    "watch": "nodemon -w ./lib --exec yarn build",
    "build": "npm-run-all -p build:cjm build:esm",
    "build:cjm": "rimraf cjm && tsc -p tsconfig.build.json -m commonjs --outDir cjm",
    "build:esm": "rimraf esm && tsc -p tsconfig.build.json -m es6 -d true --outDir esm",
    "prepublish": "yarn build",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-env": "^1.16.2",
    "clean-reactive-architecture": "^0.1.6",
    "clean-use-case": "^2.0.8",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.2.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ts-ioc-container": "^1.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-ioc-container": "^6.28.4",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-notifier": "^1.7.0"
  },
  "peerDependencies": {
    "clean-use-case": "^2.0.2",
    "react": "^17.0.2",
    "rxjs": "^7.3.0"
  },
