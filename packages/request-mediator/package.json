{
  "name": "ts-request-mediator",
  "version": "1.1.17",
  "description": "Request mediator for server",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "ibabkin <igba14@gmail.com>",
  "license": "ISC",
  "sideEffects": false,
  "main": "cjm/index.js",
  "types": "typings/index.d.ts",
  "module": "esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/request-mediator"
  },
  "homepage": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/request-mediator",
  "keywords": [
    "typescript",
    "server",
    "request-mediator"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "esm/**/*",
    "typings/**/*"
  ],
  "scripts": {
    "build:cjm": "rimraf cjm && tsc -p tsconfig.production.json --outDir cjm --module CommonJS",
    "build:esm": "rimraf esm && tsc -p tsconfig.production.json --outDir esm",
    "build:types": "rimraf typings && tsc -p tsconfig.production.json --outDir typings --emitDeclarationOnly --declaration",
    "build": "npm run build:cjm && npm run build:esm && npm run build:types",
    "watch": "nodemon --watch ./lib --exec npm run build",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "jest": "27.2.3",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.13",
    "rimraf": "3.0.2",
    "ts-ioc-container": "^32.0.6",
    "ts-jest": "27.0.5",
    "typescript": "5.4.3"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "gitHead": "ae10f302c7e0f55196b42669040735112479a854"
}
