{
    "name": "@ibabkin/ts-request-mediator",
    "version": "0.9.5",
    "description": "Request mediator for server",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "author": "ibabkin <igba14@gmail.com>",
    "license": "ISC",
    "sideEffects": false,
    "main": "cjs/index.js",
    "types": "esm/index.d.ts",
    "module": "esm/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/request-mediator"
    },
    "homepage": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/request-mediator",
    "keywords": [
        "typescript",
        "server",
        "request-mediator"
    ],
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "scripts": {
        "build:cjs": "rimraf cjs && tsc -p tsconfig.json --outDir cjs --module CommonJS --target ES5",
        "build:esm": "rimraf esm && tsc -p tsconfig.json --outDir esm",
        "build": "npm-run-all -p build:**",
        "watch": "nodemon --watch ./lib --exec npm run build",
        "test": "jest"
    },
    "devDependencies": {
        "@ibabkin/ts-constructor-injector": "^4.0.2",
        "@ibabkin/ts-ioc-container": "^23.2.6",
        "@types/jest": "27.0.2",
        "jest": "27.2.3",
        "moq.ts": "^7.3.4",
        "nodemon": "^2.0.13",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "ts-jest": "27.0.5",
        "typescript": "4.4.3"
    },
    "peerDependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "gitHead": "88835c9f71bac5beb41a9eddcd2aafc6de56d232"
}
