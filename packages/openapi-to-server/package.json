{
    "name": "@ibabkin/openapi-to-server",
    "version": "1.7.5",
    "description": "Generates server interface from openapi",
    "keywords": [
        "openapi",
        "swagger",
        "server",
        "interface",
        "types",
        "typescript"
    ],
    "author": "ibabkin <igba14@gmail.com>",
    "homepage": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/openapi-to-server",
    "license": "ISC",
    "main": "cjm/index.js",
    "types": "cjm/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/IgorBabkin/ts-ioc-container.git"
    },
    "sideEffects": true,
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "cjm/**/*",
        "precompiled/**/*"
    ],
    "bin": {
        "openapi-to-server": "cjm/useCases/openapiToServer.cli.js",
        "openapi-to-client": "cjm/useCases/openapiToClient.cli.js"
    },
    "scripts": {
        "build": "npm run clean && npm run compile",
        "test": "make-dir .generated && jest --passWithNoTests",
        "precompile": "make-dir precompiled && handlebars -m lib/templates/*.hbs -f precompiled/templates.js -c handlebars/runtime",
        "clean": "rimraf cjm",
        "compile": "tsc -p tsconfig.json --outDir cjm"
    },
    "dependencies": {
        "args": "^5.0.3",
        "axios": "^1.4.0",
        "handlebars": "^4.7.7",
        "js-yaml": "^4.1.0",
        "openapi-types": "^12.1.0",
        "typescript": "4.4.3",
        "yaml-import": "^2.0.0"
    },
    "devDependencies": {
        "@types/args": "^5.0.0",
        "@types/jest": "27.0.2",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^17.0.21",
        "jest": "27.2.3",
        "make-dir-cli": "^3.0.0",
        "rimraf": "3.0.2",
        "ts-jest": "27.0.5"
    }
}
