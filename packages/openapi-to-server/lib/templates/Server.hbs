import { Ok, Created, Route, RouteOptions } from '@ibabkin/openapi-to-server';

// Components
{{#each components.schemas}}
  export type {{@key}} = {{render_template 'JsonSchema.hbs' this}};
{{/each}}

// Routes
{{#each paths as |path url|}}
  {{#each (get_methods_obj path)}}
    {{{render_template 'ServerRoute.hbs' this}}}
  {{/each}}
{{/each}}

// Operations
export type Operations = {
{{#each paths as |path url|}}
  {{#each (get_methods_obj path)}}
    {{operationId}}: (options: RouteOptions) => {{route_name operationId}};
  {{/each}}
{{/each}}
}

