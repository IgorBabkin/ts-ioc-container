{
  "name": "reactivex-react",
  "version": "4.1.13",
  "main": "cjm/index.js",
  "module": "esm/index.js",
  "private": false,
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "keywords": [
    "rxjs",
    "react",
    "hooks",
    "reactivex"
  ],
  "license": "MIT",
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "start": "parcel ./example/index.html",
    "build": "npm-run-all -p build:cjm build:esm",
    "build:cjm": "rimraf cjm && tsc -p tsconfig.build.json -m commonjs --outDir cjm",
    "build:esm": "rimraf esm && tsc -p tsconfig.build.json -m es6 -d true --outDir esm",
    "prepublish": "yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "parceljs": "^0.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "rxjs": "^7.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/reactivex-react"
