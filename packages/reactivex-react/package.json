    "eslint-plugin-react": "^7.26.1",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
