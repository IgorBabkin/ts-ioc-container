{
    "name": "reactivex-react",
    "version": "5.2.1",
    "main": "cjs/index.js",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "module": "esm/index.js",
    "private": false,
    "sideEffects": false,
    "types": "esm/index.d.ts",
    "keywords": [
        "rxjs",
        "react",
        "hooks",
        "reactivex"
    ],
    "license": "MIT",
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "scripts": {
        "start": "webpack serve --config example/webpack.config.js",
        "build": "npm-run-all -p build:cjs build:esm",
        "build:cjs": "rimraf cjs && tsc -p tsconfig.build.json -m commonjs --outDir cjs",
        "build:esm": "rimraf esm && tsc -p tsconfig.build.json -m es6 -d true --outDir esm",
        "test": "jest"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.9",
        "@types/webpack-env": "^1.16.2",
        "clean-webpack-plugin": "^4.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-react": "^7.26.1",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.2",
        "jest": "^27.2.1",
        "moq.ts": "^7.3.4",
        "nodemon": "^2.0.13",
        "npm-run-all": "^4.1.5",
        "parceljs": "^0.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rimraf": "^3.0.2",
        "rxjs": "^7.3.0",
        "ts-jest": "^27.0.5",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.2.1",
        "typescript": "^4.4.3",
        "webpack": "^5.58.0",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.9.0",
        "webpack-dev-server": "^4.3.1",
        "webpack-notifier": "^1.14.0"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "rxjs": "^7.8.0"
    },
    "repository": {
        "type": "git",
