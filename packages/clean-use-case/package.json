{
  "name": "clean-use-case",
  "version": "2.0.8",
  "description": "Use Case Library for Clean Architecture",
  "keywords": [
    "usecase",
    "rxjs",
    "command",
    "action",
    "saga",
    "query"
  ],
  "main": "cjm/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "build": "npm-run-all -p build:cjm build:esm",
    "build:cjm": "rimraf cjm && tsc -p tsconfig.build.json -m commonjs --outDir cjm",
    "build:esm": "rimraf esm && tsc -p tsconfig.build.json -m es6 -d true --outDir esm",
    "test": "jest --passWithNoTests",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.1",
    "moq.ts": "^7.3.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "rxjs": "^7.3.0"
  },
  "author": "Igor Babkin <igba14@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/ts-ioc-container"
  },
  "gitHead": "bc12cbc43f22cc6b347037c1faff27c1a130c744"
}
