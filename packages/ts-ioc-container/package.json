{
  "name": "ts-ioc-container",
  "version": "8.1.0",
  "description": "Typescript IoC container",
  "author": "ibabkin <ibabkin@goodgamestudios.com>",
  "homepage": "https://github.com/IgorBabkin/service-locator/tree/master/packages/ts-ioc-container",
  "license": "ISC",
  "sideEffects": false,
  "main": "cjm/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "keywords": [
    "typescript",
    "ioc",
    "service-locator",
    "di"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/service-locator"
  },
  "bugs": {
    "url": "https://github.com/IgorBabkin/service-locator/issues"
  },
  "scripts": {
    "build:cjs": "rimraf cjm && tsc -p tsconfig.json --outDir cjm --module CommonJS --target ES5",
    "build:esm": "rimraf esm && tsc -p tsconfig.json --outDir esm",
    "build": "npm-run-all -p build:**",
    "watch": "nodemon --watch ./lib --exec npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "jest": "27.2.3",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.13",
    "npm-run-all": "4.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "gitHead": "22a23a7b9e7cb9260e1a8858412ddadd6421a53a",
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  }
