{
  "name": "ts-ioc-container",
  "version": "25.0.2",
  "description": "Typescript IoC container",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "ibabkin <igba14@gmail.com>",
  "homepage": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/ts-ioc-container",
  "license": "ISC",
  "sideEffects": false,
  "type": "module",
  "main": "cjm/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "keywords": [
    "dependency-injection",
    "inversion-of-control",
    "container",
    "typescript",
    "ioc",
    "di"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/ts-ioc-container"
  },
  "scripts": {
    "build:cjm": "rimraf cjm && tsc -p tsconfig.production.json --outDir cjm --module CommonJS",
    "build:esm": "rimraf esm && tsc -p tsconfig.production.json --outDir esm",
    "build:readme": "ts-node --esm scripts/generateReadme/generateReadme.ts",
    "build": "npm run build:cjm && npm run build:esm && npm run build:readme",
    "coverage": "coveralls",
    "watch": "nodemon --watch ./lib --exec npm run build",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "^18.15.3",
    "handlebars": "^4.7.7",
    "jest": "27.2.3",
    "moq.ts": "^7.3.4",
    "nodemon": "^2.0.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "^10.9.1",
    "typescript": "4.4.3"
  },
  "gitHead": "88835c9f71bac5beb41a9eddcd2aafc6de56d232"
}
