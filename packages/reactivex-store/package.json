{
  "name": "reactivex-store",
  "version": "1.0.4",
  "description": "Observable store",
  "main": "cjm/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "keywords": [
    "rxjs",
    "store",
    "observable",
    "reduce",
    "map"
  ],
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "build": "npm-run-all -p build:cjm build:esm",
    "build:cjm": "rimraf cjm && tsc -p tsconfig.build.json -m commonjs --outDir cjm",
    "build:esm": "rimraf esm && tsc -p tsconfig.build.json -m es6 -d true --outDir esm",
    "test": "jest --passWithNoTests",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "rxjs": "^7.3.0"
  },
  "author": "Igor Babkin <igba14@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/reactivex-store"
  }
}
