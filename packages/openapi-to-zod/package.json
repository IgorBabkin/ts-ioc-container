{
  "name": "@ibabkin/openapi-to-zod",
  "version": "1.2.10",
  "description": "Generates zod from openapi",
  "keywords": [
    "openapi",
    "swagger",
    "zod",
    "typescript"
  ],
  "author": "ibabkin <igba14@gmail.com>",
  "homepage": "https://github.com/IgorBabkin/ts-ioc-container/tree/master/packages/openapi-to-zod",
  "license": "ISC",
  "main": "cjm/index.js",
  "types": "cjm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/ts-ioc-container.git"
  },
  "sideEffects": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "precompiled/**/*"
  ],
  "bin": {
    "openapi-to-zod": "cjm/useCases/openapiToZod.cli.js"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "test": "make-dir .generated && jest --coverage",
    "clean": "rimraf cjm",
    "compile": "tsc -p tsconfig.json --outDir cjm",
    "precompile": "make-dir precompiled && handlebars -m lib/templates/*.hbs -f precompiled/templates.js -c handlebars/runtime"
  },
  "dependencies": {
    "args": "^5.0.3",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "yaml-import": "^2.0.0"
  },
  "devDependencies": {
    "@types/args": "^5.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "^17.0.21",
    "jest": "27.2.3",
    "make-dir-cli": "^3.0.0",
    "openapi-types": "^12.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3",
    "zod": "^3.21.4"
  }
}
