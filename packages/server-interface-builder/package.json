{
  "name": "server-interface-builder",
  "version": "0.0.0",
  "description": "Generates server interface from openapi",
  "keywords": [
    "openapi",
    "swagger",
    "server",
    "interface",
    "types",
    "typescript"
  ],
  "author": "ibabkin <igba14@gmail.com>",
  "homepage": "https://github.com/IgorBabkin/ts-ioc-container#readme",
  "license": "ISC",
  "main": "cjm/index.js",
  "types": "esm/index.d.ts",
  "module": "esm/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorBabkin/ts-ioc-container.git"
  },
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "cjm/**/*",
    "esm/**/*"
  ],
  "scripts": {
    "build": "rimraf cjm && tsc -p tsconfig.json --outDir cjm --module CommonJS --target ES5",
    "watch": "nodemon --watch ./lib --exec npm run build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "openapi-types": "^12.1.0",
    "handlebars": "^4.7.7",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "^17.0.21",
    "jest": "27.2.3",
    "nodemon": "^2.0.13",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5"
  }
}
